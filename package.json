{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public --single",
    "start:dev": "sirv public --single --dev",
    "format": "npm-run-all -c format:*",
    "format:eslint": "eslint --fix '**/*.{js,ts,svelte}'",
    "format:prettier": "prettier --write '**/*.{js,ts,svelte,json,md,css}'",
    "lint": "npm-run-all -c lint:*",
    "format:types": "tsc",
    "lint:eslint": "eslint '**/*.{js,ts,svelte}'",
    "lint:prettier": "prettier --check '**/*.{js,ts,svelte,json,md,css}'",
    "lint:types": "tsc",
    "upd-all": "npm outdated || ncu -u && npm update && npm install"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/svelte": "^1.8.0",
    "@types/downloadjs": "^1.4.1",
    "@types/jest": "^24.0.18",
    "@types/webmidi": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.18",
    "postcss-load-config": "^2.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "surge": "^0.21.3",
    "svelte": "^3.9.1",
    "svelte-preprocess": "^3.1.1",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "core-js": "^3.2.1",
    "downloadjs": "^1.4.7",
    "musenet-midi": "github:stevenwaterman/musenet-midi#dist",
    "panzoom": "^9.2.1",
    "retry-axios": "^2.1.1",
    "sirv-cli": "^0.4.4"
  }
}
